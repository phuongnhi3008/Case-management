@model NienLuan2.Models.ModelsXetXu

<div class="container-fluid">

    <div class="card mb-3">
        <div class="card-header">
            <i class="fas fa-table"></i>
            CHI TIẾT HỒ SƠ
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" cellspacing="0">
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Mã số hồ sơ:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">@Model.MA_HoSo</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Tên hồ sơ:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">@Model.Ten_VuAn</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Thẩm phán:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">@Model.HoTen_NV</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Loại vụ án:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">@Model..Ten_LoaiVA</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Trạng thái hồ sơ:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">@Model.Ten_TT</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Nội dung:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">Model.NoiDung_VA</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Loại hồ sơ:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">Model.Loai_HS</label>
                        </td>
                    </tr>
                    <tr>
                        <td><label for="inputName" class="form-control" style="border: 0px;">Ngày nhận hồ sơ:</label></td>
                        <td>
                            <label for="inputName" class="form-control" style="border: 0px; color:black;">Model.NgayNhan_HS</label>
                        </td>
                    </tr>

                        
                </table>
            </div>
        </div>
    </div>
</div>

